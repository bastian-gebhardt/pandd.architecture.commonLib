<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  
	<name>@add_project_artifacId_here@</name>

	<groupId>@add_project_groupId_here@</groupId>
    <artifactId>@add_project_artifacId_here@</artifactId>
    <version>@add_project_version_here@</version>
    
    <dependencies>
		<!-- don't remove this dependency, it's needed by pandd framework -->
	    <dependency>
	      <groupId>de.bgeb.pandd.arche</groupId>
	      <artifactId>commonlib</artifactId>
	      <version>0.1.0-SNAPSHOT</version>
	    </dependency>
    </dependencies>
    
  <!-- needed to download dependencies -->
  <repositories>  
    <repository>
      <id>download</id>
      <name>user specified repo</name>
      <url>@add_download_url_here@</url>
      <layout>default</layout>
    </repository>
  </repositories>  
  <!-- needed to upload package -->
  <distributionManagement>  
    <repository>
      <id>upload</id>
      <name>user specified repo</name>
      <url>@add_upload_url_here@</url>
      <layout>default</layout>
    </repository>
  </distributionManagement>
  
</project>
